{
  "name": "onechriscantu",
  "version": "1.0.0",
  "description": "A portfolio site for Chris Cantu",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "start": "node dist/server.js",
    "start nodemon": "nodemon -w dist dist/server.js",
    "compile": "SET NODE_ENV=production && webpack -p",
    "compile:css": "node-sass sass/style.scss sass/bundle.com.css",
    "prefix:css": "postcss sass/bundle.com.css -o sass/style.prefix.css --use autoprefixer -d",
    "compress:css": "node-sass sass/style.prefix.css static/bundle.fe913c043711f1050793.css --output-style compressed",
    "build:css": "npm-run-all compile:css prefix:css compress:css",
    "watch": "webpack-dev-server --hot --inline",
    "compile-server": "babel server --presets es2015-node4 --out-dir dist --source-maps",
    "watch-server": "babel server --presets es2015-node4 --out-dir dist --source-maps --watch",
    "dev-all": "npm run watch & npm run watch-server",
    "dev-all-hook": "npm run watch & npm run start-hook",
    "dev:watch": "webpack-dev-server --hot --inline",
    "devserver": "live-server",
    "compile:dev": "webpack",
    "lint": "eslint --fix --ext js,jsx js server webpack.config.js"
  },
  "author": "Chris Cantu",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglify": "^0.1.5",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-sass": "^2.2.1"
  }
}
